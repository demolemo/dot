#!/usr/bin/env zsh

# Easier navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Common directories
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias p="cd ~/projects"

# Git aliases
alias ga="git add"
alias gc="git checkout"
alias gcb="git checkout -b"
alias gcm="git commit -m"
alias gl="git log"
alias gs="git status"
alias gp="git push"
alias gd="git diff"
